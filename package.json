{
  "name": "pt2itp",
  "version": "12.0.1",
  "description": "Attach interpolation values given a road network and address points",
  "main": "index.js",
  "bin": {
    "pt2itp": "./index.js"
  },
  "scripts": {
    "lint": "eslint index.js test/* lib/*",
    "coverage": "istanbul cover tape test/*.js && coveralls < ./coverage/lcov.info",
    "doc": "./node_modules/documentation/bin/documentation.js lint ./lib/**",
    "test": "tape test/*.test.js",
    "pretest": "echo 'DROP DATABASE pt_test; CREATE DATABASE pt_test;' | psql -U postgres && echo 'CREATE EXTENSION postgis' | psql -U postgres pt_test"
  },
  "author": "Nicholas Ingalls <nicholas.ingalls@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "@mapbox/carmen": "24.1.3",
    "@mapbox/geocoder-abbreviations": "^2.0.0",
    "@mapbox/mbtiles": "^0.9.0",
    "@mapbox/tile-cover": "^3.0.2",
    "@mapbox/tilebelt": "^1.0.1",
    "@mapbox/title-case": "^1.2.2",
    "@turf/turf": "^4.7.3",
    "csv-stringify": "^1.0.4",
    "d3-queue": "^3.0.7",
    "diacritics": "^1.3.0",
    "express": "^4.15.3",
    "fast-csv": "^2.4.0",
    "fast-levenshtein": "^2.0.6",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "pg": "^7.3.0",
    "pg-cursor": "^1.2.0",
    "pluralize": "^7.0.0",
    "progress": "^2.0.0",
    "simple-statistics": "^5.0.0",
    "split": "^1.0.0",
    "talisman": "^0.20.0",
    "turf-line-slice-at-intersection": "^1.0.1",
    "wellknown": "^0.5.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "documentation": "^5.3.1",
    "eslint": "^4.6.1",
    "istanbul": "^0.4.0",
    "tape": "^4.7.0",
    "tmp": "0.0.33"
  },
  "eslintConfig": {
    "rules": {
      "require-jsdoc": [
        "error",
        {
          "require": {
            "FunctionDeclaration": true,
            "MethodDefinition": false,
            "ClassDeclaration": false,
            "ArrowFunctionExpression": false
          }
        }
      ],
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "space-before-function-paren": [
        2,
        "never"
      ],
      "no-tabs": 2,
      "space-in-parens": 2,
      "space-before-blocks": 2,
      "keyword-spacing": 2,
      "comma-style": 2,
      "no-lonely-if": 2,
      "no-else-return": 0,
      "new-cap": 0,
      "no-empty": 2,
      "no-new": 2,
      "no-multi-spaces": 0,
      "space-in-brackets": 0,
      "brace-style": 0,
      "quotes": 0,
      "no-underscore-dangle": 0,
      "curly": 0,
      "no-constant-condition": 0,
      "no-native-reassign": 0,
      "no-shadow": 0,
      "key-spacing": 0
    },
    "env": {
      "node": true,
      "es6": true
    }
  },
  "engines": {
    "node": "6.x.x"
  },
  "repository": {
    "type": "git"
  }
}
